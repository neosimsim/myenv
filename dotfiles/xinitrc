xautolock -cornerdelay 1 -cornerredelay 5 -time 1 -locker 'slock' -corners 0-00 &
xrdb ~/.Xresources
[ -f ~/local/Xresources ] && xrdb -merge ~/local/Xresources
xmodmap ~/.Xmodmap
xmodmap ~/.Xmodmap.querty
[ -f ~/.fehbg ] && sh ~/.fehbg
amixer set Master mute
amixer -c 0 set Headphone unmute
amixer -c 0 set Headphone 70

which 9 && 9 plumber -p $HOME/.plumbing

signal-desktop &
wire-desktop &

[ -f ~/local/xinitrc ] && . ~/local/xinitrc

exec xmonad
